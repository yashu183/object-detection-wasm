{
  "name": "yolo-object-detection",
  "version": "1.0.0",
  "description": "Real-time AI-powered object detection running entirely in the browser using YOLOv8 and ONNX Runtime Web",
  "main": "index.html",
  "scripts": {
    "start": "python3 -m http.server 8080",
    "dev": "python3 -m http.server 8080",
    "setup": "bash setup.sh",
    "export-model": "python3 export_yolo.py n",
    "serve": "npx http-server -p 8080"
  },
  "keywords": [
    "yolo",
    "object-detection",
    "ai",
    "machine-learning",
    "webassembly",
    "onnx",
    "computer-vision",
    "real-time",
    "browser"
  ],
  "author": "Yashwanth C",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.7.0"
  },
  "files": [
    "index.html",
    "yolo-detection.js",
    "export_yolo.py",
    "README.md",
    "setup.sh"
  ]
}